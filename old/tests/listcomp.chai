
def main():
    let evens = [x for x in [0 .. 40] if x % 2 == 0]
    print(evens)

    let squares  = [x ** 2 for x in [1 .. 10]]
    print(squares)

    let names = ["alice", "bob", "claire", "david", "evelyn", "frank", "grace", "hector", "ima", "jack", "kenzie"]
    let peeps = [names[i] for i in [0 .. len(names) - 1] if i % 2 == 0]
    print(peeps)

