class Student:
    # we could also assign theese initial values here instead of constructor
    var name String
    var credits Int
    var gpa Float

    def Student(name String):
        self.name = name
        self.credits = 0
        self.gpa = 0

    def print():
        println("Name:" + self.name)
        println("Credits:" + self.credits)
        println("GPA:" + self.gpa)

    def canGraduate():
        return self.credits >= 120 and self.gpa >= 2.0

    def setName(name String):
        self.name = name

def main():
    s1 = Student()
    s1.setName("Duncan Bumpkiss")
    s1.print()

