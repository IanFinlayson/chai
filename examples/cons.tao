
# returns the sum of the list, unless
# there is a 0, then it returns 0
def proccess(list [Int]) Int:
    match list:
        case [0::rest]:
            return 0
        case [x::rest]:
            return x + proccess(rest)

def main():
    let list1 = 4 :: 5 :: 0 :: [9, 8]
    print(proccess(list1))
    print(proccess([4, 5, 7, 9, 8]))

