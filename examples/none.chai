
type Option<T> = None |
                 Some of T

def slope(x0 Int, y0 Int, x1 Int, y1 Int) Option<Int>:
    if x0 != x1:
        return Some (y1 - y0) / (x1 - x0)
    else:
        return None

def main():
    match slope(10, 5, 9, 8):
        case None:
            println("Could not find slope of vertical line!")
        case Some num:
            println("The slope of the line is " + num)

