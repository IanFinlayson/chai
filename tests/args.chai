def makeWindow(title = "", width = 1024, height = 768, full = False):
    print("title is '", title, "'", sep="")
    print("width is", width)
    print("height is", height)
    print("full is", full, end="\n\n")


def main():
    makeWindow("Game", 800, 600, False)
    makeWindow()
    makeWindow("My Program")
    makeWindow(title = "Tetris", full = True)

